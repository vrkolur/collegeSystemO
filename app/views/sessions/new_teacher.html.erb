<h1>Teacher Sign In</h1>

<%=form_with model: @teacher, url: teacher_sign_in_path do |form| %>

    <div class="mb-3">
        <%= form.label :email%>
        <%= form.text_field :email,class:"form-control", placeholder:" Ex:aisha@email.com" %>
    </div>

    <div class="mb-3">
        <%= form.label :password%>
        <%= form.password_field :password,class:"form-control", placeholder: "password" %>
    </div>

    <div class="mb-3">
        <%= form.submit "Sign In", class:"btn btn-primary" %>
    </div>

<%end%>


